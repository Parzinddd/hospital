page{
    background-color: rgb(248, 248,248);
}

.line{
    height: 0.1rpx;
    width: 80vw;
    margin-left:auto;
    margin-right: auto;
    background-color:rgb(184, 180, 180);
}

.join_block{
    border-radius: 20px;
    margin: 4vw auto auto;
    background-color:white;
    width: 90vw;
    height: 20vw;
    display: flex;
    flex-direction: column;
}


.projectName{ 
    height: 15vw;
    display: flex;
    align-items: center;
    font-size: 13pt;
    margin-left: 3vw;
    float: left;
}
.join_header_right{
    float: right;
}
.goToCoUpload{
    height: 15vw;
    display: flex;
    align-items: center;
    font-size: 10pt;
    font-weight:250;
    color: #b40e0e;
    margin-right: 3vw;
}

.alreadyUpload{
    height: 15vw;
    display: flex;
    margin-right: 3vw;
    align-items: center;
    font-size: 10pt;
    font-weight:250;
    color: #32cd32;
}

.projectInfo{
    font-size: 10pt;
}

.projectDate{
    float: left;
    font-size: 10pt;
    margin-left: 3vw;
    color: rgb(107, 107, 107);
}


.projectStatus{
    margin-right: 3vw;
    float: right;
    color: rgb(0, 25, 250);
}